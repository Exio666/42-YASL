#!yasl_linux

!!
(# 1 < ) ? ("Bad argument\n" print 1 exit)
%][:5
5:#:5
(3:5 5:>) ? ("Bad argument\n" print 1 exit)
0
+

(0 %<) ? ("Bad argument\n" print 1 exit)
!
(20 %>) ? ("Result overflow\n" print 1 exit)
!

1:1
1:2
1:3

{0:=:3
2:=:3
3:>=:3}need_to_continue

3:@(
	2:=:1
	1:*:1
	1:2
	2:+:2
	need_to_continue
	)
1:print
"\n" 
print