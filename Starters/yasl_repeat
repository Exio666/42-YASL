#!yasl_linux

{
	(# 0 ==) ?
	(0 exit) :
	(1:=:2 
	2:%@(
		0:=:3
		3:print
		0:"1"
		0:eval:2
		2:-:2
		)
	0:!
	1:1
	1:+:1
	"\n"
	0:print
	repeat)
}repeat

!!
(# 2 < ) ? ("Bad argument\n" print 1 exit)
0:=:0
(0 <=) ? ("Bad argument\n" print 1 exit)
0:=:1
0:=:4
4:][:4
4:=:5
4:!
5:%@(
    (4:=:4 0:4 4:>:4) 4:? () : ("The first argument start_num must be a positive number\n":4 4:print 0 exit)
    (4:=:4 9:4 4:<:4) 4:? () : ("The first argument start_num must be a positive number\n":4 4:print 0 exit)
    4:!
    1:5
    5:-:5
    )
!
repeat